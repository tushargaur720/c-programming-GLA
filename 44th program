// 47. WACP To short a given array in accending order using bubble shot ?
#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    scanf("%d",&a[i]);
        
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
         if(a[j]>a[j+1]){
           int t=a[j];
            a[j]=a[j+1];
            a[j+1]=t;
            } 
        }
    }
    for(int i=0;i<n;i++){
     printf("%d\t",a[i]);
    }
    return 0;

}

