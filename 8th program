// 7. check wheather the character is vowel or consonant by switch case?
#include <stdio.h>
int main()
{
char ch;
printf("enter the character : ");
scanf("%c",&ch);
switch(ch)
{
    case 'a': 
    case 'e':
    case 'i': 
    case 'o': 
    case 'u':
    case 'A':
    case 'E': 
    case 'I':
    case 'O': 
    case 'U':
    printf("vowel");
    break;
    
   
     default:
     printf("consonant");
}
     return 0;
}
