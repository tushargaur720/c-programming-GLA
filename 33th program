// 35. To print reverse of the table taking n from user?
#include<stdio.h>
int main(){
    int n,table;
    printf("enter the value of n:");
    scanf("%d",&n);
    for(int i=10;i>=1;i--){
    table=n*i;
       printf("%d\n",table);
    }
    return 0;
}

// ALTERNATE METHOD
#include<stdio.h>
int main(){
int n;
printf("enter n:");
scanf("%d",&n);
for(i=10;1>=1;i--){
    printf("%d\n",i*n);
}
return 0;
}

