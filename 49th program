// 61.WACP to make two function one to scan array and one  to print 1 d array ?
#include<stdio.h>
void scanar(int *p,int n)
{
for(int i=0;i<n;i++)
scanf("%d",p+i);
}
void printer (int *p,int n)
{
for(int i=0;i<n;i++)
printf("%d",*(p+i));
}
int main()
{
int a[100];
scanar(a,5);
printer(a,5);
return 0;
}

